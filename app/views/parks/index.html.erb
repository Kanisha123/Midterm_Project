<h1>Parks#index</h1>
<p>Find me in app/views/parks/index.html.erb</p>

<table class="table">
    <thead class="thead-dark">
      <tr>
        <th>Park Name</th>
        <th>Address</th>
        <th>Park Category</th>
      </tr>
    </thead>
    <% @parks.each do |park| %>
      <tr>
        <td><%=park.name%></td>
        <td><%=park.address%></td>
        <td><%=park.category%></td>
      </tr>
    <%end%>
  </table>

