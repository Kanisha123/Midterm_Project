    <thead class="thead-dark">
      <tr>
        <th>Botanical Name</th>
        <th>Common Name</th>
      </tr>
    </thead>
    <% @trees.each do |tree| %>
      <tr>
        <td><%=tree.botanical_name%></td>
        <td><%=tree.common_name%></td>
      </tr>
    <%end%>
  </table>
